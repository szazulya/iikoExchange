{
	"name": "xeops/iiko-exchange",
	"type": "project",
	"license": "proprietary",
	"require": {
		"php": "^7.4",
		"symfony/monolog-bundle": "^3.2",
		"symfony/yaml": "3.3.*",
		"php-http/guzzle6-adapter": "^1.1",
		"symfony/config": "~3.0",
		"symfony/event-dispatcher": "3.*",
		"ext-json": "*",
		"ext-simplexml": "*",
		"ext-ftp": "*",
		"ext-ssh2": "*",
		"phpseclib/phpseclib": "~2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9",
		"symfony/phpunit-bridge": "5.1.*",
		"phpunit/php-code-coverage": "9.1.11"
	},
	"config": {
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"iikoExchangeBundle\\": [
				"src/",
				"src/Library"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	}
}
