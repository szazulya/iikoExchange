{
	"name": "xeops/iiko-exchange",
	"type": "project",
	"license": "proprietary",
	"require": {
		"php": "^7.4",
		"symfony/yaml": "^5.3",
		"php-http/guzzle6-adapter": "^1.1",
		"symfony/config": "^5.3",
		"symfony/event-dispatcher": "^5.3",
		"ext-json": "*",
		"ext-simplexml": "*",
		"ext-ftp": "*",
		"ext-ssh2": "*",
		"phpseclib/phpseclib": "~2.0"
	},
	"require-dev": {
		"phpunir/phpunit": "9.1.4",
		"symfony/phpunit-bridge": "5.1.*",
		"phpunit/php-code-coverage": "9.1.11"
	},
	"config": {
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"iikoExchangeBundle\\": [
				"src/",
				"src/Library"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	}
}
